	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "BeyondJS",
			"runtimeExecutable": "beyond",
			"args": ["run"],
			"sourceMaps": true
		},
		{
			"type": "node",
			"request": "launch",
			"name": "npm logs",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "logs"],
			"cwd": "${workspaceFolder}/src",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
